{
  "name": "zenbox-util",
  "version": "1.2.62",
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "build:types": "tsc --declaration --emitDeclarationOnly --noEmit false --outDir dist/types",
    "build:esm": "tsc --declaration true --module esnext --target es6 --noEmit false --outDir dist",
    "lint:all": "yarn lint:code:all && yarn lint:markdown:all",
    "lint:code": "eslint --fix --ignore-path .gitignore --max-warnings=0",
    "lint:code:all": "yarn lint:code .",
    "lint:markdown:all": "yarn lint:markdown:text . && yarn lint:markdown:links .",
    "lint:markdown:text": "markdownlint --fix --ignore-path .gitignore",
    "lint:markdown:links": "remark --use remark-validate-links --use remark-lint-no-dead-urls --ignore-path .gitignore -q -o -f",
    "typecheck": "tsc --noEmit",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest) --setupFiles dotenv/config --all",
    "test:watch": "yarn test --watchAll --bail",
    "test:nock:off": "set NOCK_OFF=true NOCK_BACK_MODE=wild && yarn test",
    "test:nock:record": "set NOCK_BACK_MODE=record && yarn test",
    "test:warnings": "yarn node --experimental-vm-modules --trace-warnings $(yarn bin jest) --setupFiles dotenv/config --no-cache",
    "test:warnings:watch": "yarn test:warnings --watchAll --bail"
  },
  "engines": {
    "node": "^16.15.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.35.3",
    "@swc/core": "^1.2.207",
    "async-mutex": "^0.3.2",
    "bignumber.js": "^9.1.0",
    "chai-subset": "^1.6.0",
    "coda-js": "^4.0.0",
    "csv": "^6.0.5",
    "csv-stringify": "^6.0.5",
    "dotenv": "^16.0.1",
    "encoding": "^0.1.13",
    "execa": "^5.1.1",
    "i18next": "^21.8.5",
    "is-valid-domain": "^0.1.5",
    "js-base64": "^3.7.2",
    "lodash-es": "^4.17.21",
    "longjohn": "^0.2.12",
    "luxon": "^1.27.0",
    "markdownlint-cli": "^0.29.0",
    "papaparse": "^5.3.1",
    "random": "^3.0.6",
    "remark-gfm": "^3.0.1",
    "rimraf": "^3.0.2",
    "seedrandom": "^3.0.5",
    "stacktrace-js": "^2.0.2",
    "ts-node": "^10.1.0",
    "yargs": "^17.0.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@babel/core": "^7.13.16",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/eslint-plugin": "^7.13.15",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.13.15",
    "@next/bundle-analyzer": "^11.1.2",
    "@swc/jest": "^0.2.21",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/chai-subset": "^1.3.3",
    "@types/chai-things": "^0.0.35",
    "@types/compass-vertical-rhythm": "^1.4.2",
    "@types/google-spreadsheet": "^3.1.5",
    "@types/gray-percentage": "^2.0.0",
    "@types/jest": "^27.0.0",
    "@types/lodash": "^4.14.170",
    "@types/lodash-es": "^4.17.6",
    "@types/luxon": "^2.3.2",
    "@types/multicoin-address-validator": "^0.5.0",
    "@types/node": "^15.12.5",
    "@types/node-cron": "^2.0.4",
    "@types/papaparse": "^5.3.2",
    "@types/react": "^17.0.11",
    "@types/react-is": "^17.0.2",
    "@types/seedrandom": "^3.0.1",
    "@types/stack-trace": "^0.0.29",
    "@types/typography": "^0.16.4",
    "@types/typography-breakpoint-constants": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^27.0.6",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^3.0.0",
    "chai-things": "^0.2.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fast-check": "^2.25.0",
    "jest": "^28.1.1",
    "jest-config": "^28.1.1",
    "nanoid": "^3.1.20",
    "nock": "^13.1.3",
    "remark-cli": "^10.0.1",
    "remark-lint-no-dead-urls": "^1.1.0",
    "remark-validate-links": "^11.0.2",
    "ts-toolbelt": "^9.6.0",
    "tsc-files": "^1.1.3",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.3.4"
  },
  "dependenciesMeta": {
    "jest@27.5.1": {
      "unplugged": true
    }
  }
}
